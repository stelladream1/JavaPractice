import java.util.Scanner;
public class prca {


	public static void main(String[] args) {
		
		Scanner scanner = new Scanner(System.in);
		System.out.print("입력:");
		int a = scanner.nextInt();
		int b = scanner.nextInt();
		int c = scanner.nextInt();
		int val= 0;
		
	if(a>b&&a>c)
	{
		if(a*a == b*b + c*c)
			System.out.print("삼각형");
	}
	if(b>a&&b>c)
	{
		if(b*b == b*b + c*c)
			System.out.print("삼각형");
	}
	if(c>a&&c>b)
	{
		if(c*c == a*a + b*b)
			System.out.print("삼각형");
	}
		
		
	}
}
