import java.util.Scanner;
public class prca {


	public static void main(String[] args) {
		
		Scanner scanner = new Scanner(System.in);
		System.out.print("입력:");
		int a = scanner.nextInt();
		int b = scanner.nextInt();
		int c = scanner.nextInt();
		int val= 0;
		
		if(a>b&&c>a||a>c&&b>a)
			System.out.print(a);
			
		
		if(b>a&&c>b||b>c&&a>b)
			System.out.print(b);
		
		
		
		if(c>b&&a>c||c>a&&b>c)
			System.out.print(c);
		
		
		
		
	}
}
